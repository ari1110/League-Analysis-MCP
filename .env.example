# Yahoo App Credentials
# Get these from https://developer.yahoo.com/apps/
YAHOO_CONSUMER_KEY=your_consumer_key_here
YAHOO_CONSUMER_SECRET=your_consumer_secret_here

# Optional: Pre-generated Access Token (for private leagues)
YAHOO_ACCESS_TOKEN_JSON={"access_token":"your_token","refresh_token":"your_refresh","token_type":"bearer"}

# Default League Configuration
DEFAULT_LEAGUE_ID=123456
DEFAULT_GAME_CODE=nfl

# Server Configuration
MCP_SERVER_NAME=league-analysis-mcp
MCP_SERVER_VERSION=0.1.0

# Caching Configuration (optional)
CACHE_TTL_SECONDS=300
ENABLE_HISTORICAL_CACHE=true